SPATH       := $($(SONIC_CONFIG_ENGINE_PY2)_SRC_PATH)
DEP_FILES   := $(SONIC_COMMON_FILES_LIST) rules/sonic-config.mk rules/sonic-config.dep   
DEP_FILES   += $(SONIC_COMMON_BASE_FILES_LIST)
